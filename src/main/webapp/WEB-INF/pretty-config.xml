<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.0" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.3.0
                                        http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.0.xsd">
   <url-mapping id="home"> 
       <pattern value="/" /> 
       <view-id value="/home.jsf" />
   </url-mapping>
    
   <url-mapping id="search"> 
       <pattern value="/search" /> 
       <view-id value="/search.jsf" />
   </url-mapping> 
    
   <url-mapping id="profile"> 
       <pattern value="/profile" /> 
       <view-id value="/profile.jsf" />
       <action>#{userSession.load()}</action>
   </url-mapping>
   
   <url-mapping id="exchange"> 
       <pattern value="/exchange/#{id : giftExchangeBean.id}" /> 
       <view-id value="/exchange.jsf" />
       <action>#{giftExchangeBean.load}</action>
   </url-mapping>
   
   <url-mapping id="wishlist"> 
       <pattern value="/wishlist/#{id : wishlistBean.id}" /> 
       <view-id value="/wishlist.jsf" />
       <action>#{wishlistBean.load}</action>
   </url-mapping>
   
   <url-mapping id="wishlistView"> 
       <pattern value="/view/#{id : wishlistViewBean.id}" /> 
       <view-id value="/wishlistView.jsf" />
       <action>#{wishlistViewBean.load}</action>
   </url-mapping>
</pretty-config>